{
    "json.schemas": [
        {
            "fileMatch": ["**/*.fare-schedule.config.json"],
            "url": ".schemas/fare-schedule.schema.json"
        },
        {
            "fileMatch": ["**/*.service.config.json", "**/*.service.json"],
            "url": ".schemas/service.schema.json"
        },
        {
            "fileMatch": [
                "**/*.inparam.config.json",
                "**/*.input-parameter.config.json"
            ],
            "url": ".schemas/input-parameter.schema.json"
        },
        {
            "fileMatch": ["**/*.inparams.config.json"],
            "url": ".schemas/inparams.schema.json"
        },
        {
            "fileMatch": [
                "**/*.table-definition.config.json",
                "**/*.table-def.config.json",
                "**/*.table.json"
            ],
            "url": ".schemas/table-definition.schema.json"
        },
        {
            "fileMatch": ["**/*.auth.config.json"],
            "url": ".schemas/client-auth.schema.json"
        },
        {
            "fileMatch": ["**/*.report.json"],
            "url": ".schemas/report-definition.schema.json"
        }
    ],
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.activeIconPack": "angular",
    "material-icon-theme.files.associations": {
        "*.ktc.ts": "angular-component",
        "*.ktc.html": "angular-resolver",
        "*.ktc.scss": "angular-directive",
        "*.config.json": "nest-pipe",
        "*.contract.ts": "capacitor",
        "*.decorator.ts": "edge",
        "*.e2e.ts": "test-ts",
        "*.entity.ts": "gradle",
        "*.filter.ts": "nest-filter",
        "*.socket.ts": "nest-filter",
        "*.ng-provider.ts": "angular-pipe",
        "*.ng-socket.ts": "angular-resolver",
        "*.gateway.config.ts": "nest-pipe",
        "*.gateway.controller.ts": "nest-controller",
        "*.gateway.factory.ts": "nest-decorator",
        "*.gateway.guard.ts": "nest-guard",
        "*.gateway.module.ts": "nest-module",
        "*.gateway.service.ts": "nest-service",
        "*.cache.provider.ts": "nest-filter",
        "*.guard.ts": "nest-guard",
        "*.inserter.ts": "database",
        "*.nest.config.ts": "nest-pipe",
        "*.nest.controller.ts": "nest-controller",
        "*.nest.factory.ts": "nest-decorator",
        "*.nest.guard.ts": "nest-guard",
        "*.nest.module.ts": "nest-module",
        "*.nest.service.ts": "nest-service",
        "*.provider.ts": "nest-gateway",
        "*.report.json": "contributing",
        "*.schema.ts": "gradle",
        "*.service.json": "database",
        "*.strategy.ts": "nest-middleware",
        "*.table.json": "table",
        "*.types.ts": "tsconfig",
        "*.util.ts": "architecture",
        "*.utils.ts": "architecture",
        ".editorconfig": "settings",
        "angular.json": "angular",
        "proxy.conf.json": "apiblueprint",
        "tsconfig.build.json": "parcel",
        "tsconfig.json": "typescript",
        "tsconfig.spec.json": "test-ts"
    },
    "material-icon-theme.folders.associations": {
        "tables": "mappings",
        "interceptors": "middleware",
        "services": "controller",
        "host": "app",
        "utils": "tools",
        ".schemas": "serverless",
        "entities": "gradle",
        "inserters": "database",
        ".config": "config",
        ".old": "archive",
        "client-e2e": "coverage",
        "oracle-middleware": "middleware",
        "gateway": "connection",
        "auth0": "secure",
        "microservices": "controller",
        "pg": "gradle",
        "rxjs": "redux-reducer",
        "websocket": "connection",
        "deps": "include",
        "lib": "src",
        "config-manager": "config"
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "typescript.suggest.completeFunctionCalls": false,
    "typescript.suggest.completeJSDocs": true,
    "typescript.preferences.quoteStyle": "single",
    "typescript.format.semicolons": "insert",
    "typescript.format.insertSpaceBeforeFunctionParenthesis": false,
    "typescript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": false,
    "prettier.enable": true,
    "prettier.requireConfig": true,
    "prettier.configPath": "./.prettierrc",
    "prettier.ignorePath": "./.prettierignore",
    "typescript.tsdk": "node_modules/typescript/lib",
    "prettier.singleQuote": true,
    "prettier.trailingComma": "none",
    "prettier.singleAttributePerLine": true,
    "prettier.tabWidth": 4,
    "prettier.jsxSingleQuote": true,
    "prettier.bracketSameLine": true,
    "references.preferredLocation": "peek",
    "html.format.indentInnerHtml": true,
    "html.format.indentHandlebars": true,
    "html.format.wrapAttributes": "force",
    "search.exclude": {
        "**/deps": true,
        ".legacy": true
    },
    "search.followSymlinks": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    }
}
